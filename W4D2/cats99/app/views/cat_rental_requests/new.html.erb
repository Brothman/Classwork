<form class="" action= <%= cat_rental_requests_url %> method="post">
  <select name="cat_rental_request[cat_id]">
    <% @cats = Cat.all %>
    <% @cats.each do |cat| %>
    <option value= <%= cat.id %> > <%= cat.name %> </option>
    <% end %>
  </select>

  <label> Start Date:
    <input type="date" name="cat_rental_request[start_date]" value="">
  </label>
  <label> End Date:
    <input type="date" name="cat_rental_request[end_date]" value="">
  </label>


  <input type="submit" name="" value="Create Rental Request">
</form>
